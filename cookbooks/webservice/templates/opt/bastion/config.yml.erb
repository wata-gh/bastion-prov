:app:
  :name: <%= node[:bastion][:app][:name] %>
:aws_config:
  :access_key_id: <%= node[:bastion][:aws_config][:access_key_id] %>
  :secret_access_key: <%= node[:bastion][:aws_config][:secret_access_key] %>
  :region: <%= node[:bastion][:aws_config][:region] %>
:security_group:
  :bastion:
    :group_id: <%= node[:bastion][:security_group][:bastion][:group_id] %>
    :defaults: <%= node[:bastion][:security_group][:bastion][:defaults] %>
  :webservice:
    :group_id: <%= node[:bastion][:security_group][:webservice][:group_id] %>
    :defaults: <%= node[:bastion][:security_group][:webservice][:defaults] %>
